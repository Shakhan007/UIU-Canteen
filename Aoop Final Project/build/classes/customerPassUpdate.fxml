<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="458.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CustomerPassUpdateController">
   <children>
      <Button fx:id="updatePassBtn" layoutX="215.0" layoutY="259.0" mnemonicParsing="false" onAction="#updatebtnpushed" text="Update Password" textFill="#20f820">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Button>
      <PasswordField fx:id="newPassPf" alignment="BOTTOM_CENTER" layoutX="215.0" layoutY="184.0" prefHeight="45.0" prefWidth="171.0" promptText="New Password">
         <font>
            <Font size="18.0" />
         </font></PasswordField>
      <Label fx:id="messageLabel" layoutX="205.0" layoutY="344.0" prefHeight="39.0" prefWidth="191.0" textAlignment="CENTER" />
      <Button layoutX="514.0" layoutY="398.0" mnemonicParsing="false" onAction="#backbtnpushed" text="Back" textFill="#11f847">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Button>
      <Button layoutX="263.0" layoutY="79.0" mnemonicParsing="false" onAction="#checkbtnpushed" text="Check" textFill="#1cff07">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Button>
      <TextField fx:id="idTf" alignment="BOTTOM_CENTER" layoutX="196.0" layoutY="21.0" prefHeight="39.0" prefWidth="208.0" promptText="Enter your Id">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Label fx:id="checkLabel" layoutX="190.0" layoutY="132.0" prefHeight="33.0" prefWidth="221.0" textAlignment="CENTER" />
   </children>
</AnchorPane>
